*/

#include <stdio.h> // printf(), scanf()

// Function definition
int multiplication(int x, int y) {
    int ans;
    ans = x * y;
    return ans;
}

int main() {
    // Variable declaration
    int result, result_1;

    // Function calling
    result = multiplication(10, 20);
    result_1 = multiplication(15, 25);

    printf("The product of 10 and 20 is %d \n", result);
    printf("The product of 15 and 25 is %d \n", result_1);

    return 0;
}
